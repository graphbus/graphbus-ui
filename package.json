{
  "name": "graphbus-ui",
  "version": "0.1.5+6cabbd6",
  "description": "Electron UI for GraphBus agent orchestration with natural language control",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start:dir": "echo 'Usage: npm run start:dir /path/to/directory' && echo 'Or: npm start -- --dir=/path/to/directory'",
    "build": "electron-builder",
    "dev": "electron . --enable-logging",
    "test:e2e": "playwright test",
    "version:update": "node scripts/update-version.js",
    "version:show": "node -e \"console.log(require('./package.json').version)\"",
    "precommit": "npm run version:update",
    "prepush": "npm run version:update && git add package.json"
  },
  "keywords": [
    "graphbus",
    "electron",
    "agents",
    "orchestration",
    "nlp"
  ],
  "author": "GraphBus",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "playwright": "^1.56.1",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.69.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@xterm/xterm": "^5.5.0",
    "codemirror": "^6.0.2",
    "openai": "^4.32.0",
    "python-shell": "^5.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-xterm": "^2.0.4",
    "reactflow": "^11.11.4",
    "ws": "^8.16.0",
    "xterm": "^5.3.0",
    "yaml": "^2.3.0"
  },
  "build": {
    "appId": "com.graphbus.ui",
    "productName": "GraphBus UI",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns"
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer.js",
      "python_bridge.js",
      "index.html",
      "styles.css",
      "assets/**/*"
    ]
  }
}
